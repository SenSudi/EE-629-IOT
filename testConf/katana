server {
    listen 80;
    server_name 192.168.33.10;
    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/ubuntu/katana;
    }
    location /media/ {
        root /home/ubuntu;
    }

    location / {
        include         uwsgi_params;
        uwsgi_pass      unix:/run/uwsgi/katana.sock;
    }
}
